<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= category_name %></title>
  </head>
  <body>
    <%- include('./partials/nav') %>

    <h1><%= category_name %></h1>

    <% category.forEach((item) => { %>

    <p>
      Name: <%= item.name %> Price: <%= item.price %> Rating: <%=item.rating%>
      Details: <%=item.details%>
    </p>

    <% }) %>
  </body>
</html>
